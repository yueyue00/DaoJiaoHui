<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.gj.gaojiaohui.activity.LanguageActivity" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#384C67" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/about_us_title"
            android:textColor="#fff"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/about_us_fanhui"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:src="@drawable/home_btn_back_nor" />
    </RelativeLayout>
	<WebView 
	    android:id="@+id/about_us_webview"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
	    />
</LinearLayout>
